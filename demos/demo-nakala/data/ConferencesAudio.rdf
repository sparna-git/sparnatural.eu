         <dc:title xml:lang="fr">Leçons magistrales : La transmission de la musique antique</dc:title>
         <dcterms:created>2005</dcterms:created>
         <dcterms:extent>39 min 35 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0777908"/>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0146812"/>
