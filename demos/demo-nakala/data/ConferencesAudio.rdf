         <dc:title xml:lang="fr">Leçons magistrales : révolutions politiques et musicales : le cas Beethoven</dc:title>
         <dcterms:created>2003</dcterms:created>
         <dcterms:extent>51 min 30 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC1/0038954"/>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0777908"/>
