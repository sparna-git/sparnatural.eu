         <dc:title xml:lang="fr">L'opéra au XIXe siècle, de A à Z : Le Moyen Age en scène : prétexte romantique ou enjeu politique</dc:title>
         <dcterms:created>2020</dcterms:created>
         <dcterms:extent>2h 8 min 3 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/1049068"/>
