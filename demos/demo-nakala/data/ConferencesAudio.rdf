         <dc:title xml:lang="fr">Leçons magistrales : l'écoute bien tempérée</dc:title>
         <dcterms:created>2004</dcterms:created>
         <dcterms:extent>1h 2 min</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0777908"/>
