         <dc:title xml:lang="fr">Opéra baroque : Opéra des Lumières, de A à Z : Stylisation de la danse</dc:title>
         <dcterms:created>2018</dcterms:created>
         <dcterms:extent>2h 2 min 58 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/1049068"/>
