         <dc:title xml:lang="fr">Leçons magistrales : Initier à la musique</dc:title>
         <dcterms:created>2007</dcterms:created>
         <dcterms:extent>1h 1 min</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0777908"/>
