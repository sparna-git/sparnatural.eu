         <dc:title xml:lang="fr">Opéra baroque : Opéra des Lumières, de A à Z : Redécouverte du baroque par les compositeurs du XXe siècle</dc:title>
         <dcterms:created>2019</dcterms:created>
         <dcterms:extent>1h 55 min 53 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/1049068"/>
