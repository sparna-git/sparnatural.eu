         <dc:title xml:lang="fr">Leçons magistrales : La petite phrase musicale : de la mémoire à l'essence des choses</dc:title>
         <dcterms:created>2005</dcterms:created>
         <dcterms:extent>47 min 40 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0777908"/>
