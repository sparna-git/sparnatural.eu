         <dc:title xml:lang="fr">Leçons magistrales : Musique savante, musique populaire : une transmission ?</dc:title>
         <dcterms:created>2004</dcterms:created>
         <dcterms:extent>46 min 4 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0777908"/>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/0145872"/>
