         <dc:title xml:lang="fr">Opéra baroque : Opéra des Lumières, de A à Z : L'opéra français : miroir de la monarchie ou espace de contestation</dc:title>
         <dcterms:created>2018</dcterms:created>
         <dcterms:extent>2h 22 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/1049068"/>
