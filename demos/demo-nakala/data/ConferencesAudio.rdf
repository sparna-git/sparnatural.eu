         <dc:title xml:lang="fr">Opéra baroque : Opéra des Lumières, de A à Z : Crimes, cataclysmes et maléfices</dc:title>
         <dcterms:created>2018</dcterms:created>
         <dcterms:extent>1h 59 min 33 sec</dcterms:extent>
         <dc:subject rdf:resource="http://fake.philharmoniedeparis.fr/AIC90/1049068"/>
